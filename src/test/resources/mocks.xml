<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	    	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- 
		Etape 1 - Replacer le bean précédant par un mock Mockito
		On peut conserver les annotations @Autowired si on passe par une factory comme
		MockFactory 
	-->
	<bean id="javaMailSender" class="com.francetelecom.devdays.MockFactory">
	  <property name="type" value="org.springframework.mail.javamail.JavaMailSender" />
	</bean>
	
</beans>
